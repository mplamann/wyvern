module def stdout(python : Python)

import python:sys
import wyvern.String

type Printable = Unit

type PySysStdout
  def write(text : String) : Unit
type Sys
  val stdout : PySysStdout

val sys : Sys = sys

def print(text : String) : Unit = sys.stdout.write(text)
def printInt(n : Int) : Unit = sys.stdout.write(String.ofInt(n))
def println() : Unit = sys.stdout.write("\n")